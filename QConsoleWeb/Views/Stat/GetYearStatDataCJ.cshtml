

<canvas id="YearChart"> </canvas>

<script>
    var YearStatDataCJ = @Html.Raw(ViewBag.YearStatDataCJ);
    var PeriodString = "@Html.Raw(ViewBag.Periodstring)";

    var options = {
        legend: {
            labels: {
                usePointStyle: true,
                fontSize: 10,
                padding: 5
            },
            position: 'right'
        },
        title: {
            text: 'Количество новых объектов за период ' + PeriodString,
            display: true
        },
        plugins: {
            filler: {
                propagate: true
            }
        },
        tooltips: {
            mode: 'point'
        },
        responsive: true,
        maintainAspectRatio: false
    };

    var chart = new Chart('YearChart', {
        type: 'line',
        data: YearStatDataCJ,
        options: options
    });
</script>

